apiVersion: elemental.cattle.io/v1beta1
kind: ManagedOSImage
metadata:
  name: workers-upgrade
  namespace: fleet-default
spec:
  managedOSVersionName: "v2.0.2"
  cordon: true
  nodeSelector:
    matchExpressions:
    - key: cores
      operator: In
      values: [ "2" ]
  clusterTargets:
    - clusterName: volcano
